{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|svg|ico|webp))",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.glb)",
      "dest": "/$1",
      "headers": {
        "Content-Type": "model/gltf-binary"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
